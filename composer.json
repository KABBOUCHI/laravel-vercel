{
    "name": "kabbouchi/laravel-vercel",
    "description": "",
    "keywords": [
        "kabbouchi",
        "laravel-vercel"
    ],
    "homepage": "https://github.com/kabbouchi/laravel-vercel",
    "license": "MIT",
    "authors": [
        {
            "name": "Georges KABBOUCHI",
            "email": "georges.kabbouchi@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4",
        "illuminate/contracts": "^7.0|^8.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16"
    },
    "autoload": {
        "psr-4": {
            "KABBOUCHI\\LaravelVercel\\": "src/"
        }
    },
    "scripts": {
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "KABBOUCHI\\LaravelVercel\\LaravelVercelServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
